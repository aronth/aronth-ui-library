    }
  },
  "types": "./dist/main.d.ts",
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^2.3.4",
    "path": "^0.12.7",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.15",
    "vite-plugin-dts": "^2.1.0",
    "vue-tsc": "^0.34.7"
  }
